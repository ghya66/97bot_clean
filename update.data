CREATE TABLE `tb_webapp` (
  `id` int(11) NOT NULL,
  `key` varchar(16) NOT NULL COMMENT 'key',
  `name` varchar(32) NOT NULL COMMENT '小程序名称',
  `byuser` varchar(16) NOT NULL COMMENT '作者',
  `version` int(4) NOT NULL COMMENT '版本',
  `usdt` decimal(12,2) NOT NULL COMMENT '售价',
  `day` int(4) NOT NULL COMMENT '时长',
  `image` json NOT NULL COMMENT '预览图',
  `text` text NOT NULL COMMENT '说明',
  `newtext` text NOT NULL COMMENT '新版本说明',
  `create_time` int(10) NOT NULL COMMENT '创建时间',
  `update_time` int(10) NOT NULL COMMENT '最后更新'
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;

ALTER TABLE `tb_webapp`
  ADD PRIMARY KEY (`id`),
  ADD KEY `app` (`key`);


ALTER TABLE `tb_webapp`
  MODIFY `id` int(11) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=1;
COMMIT;
